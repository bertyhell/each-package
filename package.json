{
  "name": "each-package",
  "version": "0.3.9",
  "description": "Run commands in each package folder starting with cwd skipping node_modules folders",
  "keywords": [
    "each",
    "package",
    "npm",
    "install",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/each-package.git"
  },
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "ep": "./bin/each-package.js"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "dtd \"npm run lint\" \"depcheck\"",
    "test": "mocha-compat test/spec/**/*.test.js  --no-timeouts"
  },
  "dependencies": {
    "cross-spawn-cb": "^0.4.1",
    "exit": "^0.1.2",
    "fs-iterator": "^4.1.0",
    "getopts-compat": "^2.2.5",
    "object-assign": "^4.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cr": "^0.1.0",
    "depcheck": "^1.4.3",
    "dis-dat": "^0.1.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "is-version": "^0.2.0",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.7.1"
  },
  "engines": {
    "node": ">=0.8"
  }
}
